{
  "name": "gulp-tasks",
  "description": "Gulp build for .net-based systems, should work with little or no changes",
  "scripts": {
    "test": "mocha spec/**/*.spec.js",
    "preautotest-once": "node -e \"console.log('Tests started at: ', new Date());\"",
    "autotest-once": "mocha spec/**/*.spec.js --reporter mocha-yar",
    "autotest": "nodemon -q -x \"run-s --silent autotest-once\""
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "debug": "^2.6.1",
    "event-stream": "^3.3.1",
    "gulp": "^3.9.0",
    "gulp-debug": "^3.1.0",
    "gulp-help": "^1.6.1",
    "gulp-msbuild": "^0.4.0",
    "gulp-npm-run": "0.0.3",
    "gulp-nunit-runner": "^1.2.1",
    "jasmine-core": "^2.3.4",
    "jsdom": "^10.1.0",
    "mkdirp": "0.5.1",
    "mocha": "^3.2.0",
    "mocha-yar": "^1.0.12",
    "nodemon": "^1.10.2",
    "npm-run-all": "^4.0.2",
    "proxyquire": "^1.7.10",
    "q": "^1.4.1",
    "request": "^2.75.0",
    "require-dir": "^0.3.0",
    "requirejs": "^2.1.18",
    "run-sequence": "^1.1.0",
    "through2": "^2.0.0",
    "which": "^1.2.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fluffynuts/gulp-tasks"
  },
